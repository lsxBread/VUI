{
  "name": "bread-vui",
  "version": "0.0.3",
  "description": "This is a UI framework made of Vue.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx parcel index.html --no-cache",
    "dev-test": "parcel watch test/* --no-cache & karma start",
    "test": "parcel build test/* --no-cache --no-minify  --no-source-maps && karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsxBread/VUI.git"
  },
  "keywords": [
    "Vue",
    "UI"
  ],
  "author": "Leo Liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lsxBread/VUI/issues"
  },
  "homepage": "https://github.com/lsxBread/VUI#readme",
  "dependencies": {
    "src": "^1.1.2",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.3.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^3.1.3",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.10.3",
    "sass": "^1.15.1",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.5.17"
  },
